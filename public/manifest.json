{
    "manifest_version": 3,
    "name": "Tab overlay",
    "icons":{
        "32":"icon.jpg"
    },
    "description": "A basic example extension to learn from it",
    "version": "1.0.0",
    "action":{
        "default_popup": "index.html"
    },
    "options_page": "index.html",
    "background":{
        "service_worker": "service_worker.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions":[
        "tabs",
        "activeTab",
        "scripting",
        "history"
    ],
    "host_permissions": [
    "http://*/*",
    "https://*/*",
    "<all_urls>"
    ]
}